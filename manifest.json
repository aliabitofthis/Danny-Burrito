{
    "manifest_version": 3,
    "name": "Danny Devito Image Replacer",
    "version": "1.0",
    "description": "Automatically replaces all images on web pages with random Danny Devito images.",
    "permissions": [],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_idle"
        }
    ],
    "icons": {
  "16": "icon16.png",
  "32": "icon48.png",
  "64": "icon64.png",
  "128": "icon128.png"
    },
    "action": {
        "default_icon": {
            "48": "icon48.png"
        }
    }
}
