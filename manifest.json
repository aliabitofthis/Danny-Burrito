{
    "manifest_version": 3,
    "name": "Danny Devito Image Replacer",
    "version": "1.0",
    "description": "Automatically replaces all images on web pages with random Danny Devito images.",
    "permissions": [],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_idle"
        }
    ],
    "icons": {
  "16": "https://github.com/aliabitofthis/Danny-Burrito/blob/main/icon16.png?raw=true",
  "48": "https://github.com/aliabitofthis/Danny-Burrito/blob/main/icon48.png?raw=true",
  "64": "https://github.com/aliabitofthis/Danny-Burrito/blob/main/icon64.png?raw=true",
  "128": "https://github.com/aliabitofthis/Danny-Burrito/blob/main/icon128.png?raw=true"
    },
    "action": {
        "default_icon": {
            "48": "https://github.com/aliabitofthis/Danny-Burrito/blob/main/icon48.png?raw=true"
        }
    }
}
